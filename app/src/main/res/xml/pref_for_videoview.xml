<?xml version="1.0" encoding="utf-8"?><!-- for Original Demo application -->


<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:attr="http://schemas.android.com/apk/res/com.nexstreaming.app.nexplayersample">

    <PreferenceScreen android:title="Basic">
        <CheckBoxPreference
            android:defaultValue="false"
            android:key="useUDP"
            android:summary="@string/pref_useUDP_desc"
            android:summaryOff="@string/pref_useUDP_off"
            android:summaryOn="@string/pref_useUDP_on"
            android:title="@string/pref_useUDP"></CheckBoxPreference>
        <CheckBoxPreference
            android:defaultValue="false"
            android:key="useExternalPD"
            android:summary="@string/pref_useExternalPD_desc"
            android:summaryOff="@string/pref_useExternalPD_off"
            android:summaryOn="@string/pref_useExternalPD_on"
            android:title="@string/pref_useExternalPD"></CheckBoxPreference>

        <CheckBoxPreference
            android:defaultValue="true"
            android:key="enabledtWebvtt"
            android:summaryOff="@string/pref_prevent_webvtt_off"
            android:summaryOn="@string/pref_prevent_webvtt_on"
            android:title="@string/pref_prevent_webvtt"></CheckBoxPreference>
        <com.nexstreaming.app.apis.PreferenceEditTextFormatted
            android:defaultValue="@string/pref_subtitle_download_path_default_value"
            android:key="@string/pref_subtitle_download_path_key"
            android:title="@string/pref_subtitle_download_path"

            ></com.nexstreaming.app.apis.PreferenceEditTextFormatted>

        <CheckBoxPreference
            android:defaultValue="false"
            android:key="ignoreTextmode"
            android:summaryOff="@string/pref_cea608_ignore_textmode_on"
            android:summaryOn="@string/pref_cea608_ignore_textmode_off"
            android:title="@string/pref_cea608_ignore_textmode"></CheckBoxPreference>

        <com.nexstreaming.app.apis.ListPrefWithSummary
            android:defaultValue="2"
            android:entries="@array/pref_replay_mode_entries"
            android:entryValues="@array/pref_replay_mode_values"
            android:key="replayMode"
            android:summary="summary"
            android:title="@string/pref_replayMode"></com.nexstreaming.app.apis.ListPrefWithSummary>

        <CheckBoxPreference
            android:defaultValue="true"
            android:key="WebvttWaitOpen"
            android:summaryOff="@string/pref_webvtt_waitopen_off"
            android:summaryOn="@string/pref_webvtt_waitopen_on"
            android:title="@string/pref_webvtt_waitopen"></CheckBoxPreference>

        <CheckBoxPreference
            android:defaultValue="false"
            android:key="HLSRunModeStable"
            android:summaryOff="@string/pref_hls_runmode_stable_off"
            android:summaryOn="@string/pref_hls_runmode_stable_on"
            android:title="@string/pref_hls_runmode_stable"></CheckBoxPreference>

        <com.nexstreaming.app.apis.PreferenceEditTextFormatted
            android:defaultValue=""
            android:key="@string/pref_prefer_language_audio_key"
            android:summary="summary"
            android:title="@string/pref_prefer_language_audio_title"

            ></com.nexstreaming.app.apis.PreferenceEditTextFormatted>

        <com.nexstreaming.app.apis.PreferenceEditTextFormatted
            android:defaultValue=""
            android:key="@string/pref_prefer_language_text_key"
            android:summary="summary"
            android:title="@string/pref_prefer_language_text_title"

            ></com.nexstreaming.app.apis.PreferenceEditTextFormatted>

        <com.nexstreaming.app.apis.PreferenceEditTextFormatted
            android:defaultValue="0"
            android:key="@string/pref_start_nearest_bw_key"
            android:summary="summary"
            android:title="@string/pref_start_nearest_bw_title"

            ></com.nexstreaming.app.apis.PreferenceEditTextFormatted>

        <com.nexstreaming.app.apis.PreferenceEditTextFormatted
            android:defaultValue="0"
            android:key="@string/pref_start_sec_key"
            android:summary="summary"
            android:title="@string/pref_start_sec_title"

            ></com.nexstreaming.app.apis.PreferenceEditTextFormatted>

        <CheckBoxPreference
            android:defaultValue="true"
            android:key="@string/pref_enableAudioOnlyTrack_key"
            android:summaryOff="@string/pref_enableAudioOnlyTrack_off"
            android:summaryOn="@string/pref_enableAudioOnlyTrack_on"
            android:title="@string/pref_enableAudioOnlyTrack_title"></CheckBoxPreference>

        <com.nexstreaming.app.apis.SliderPreference
            interval="1"
            max="10240"
            min="0"
            unitLabel="kbps"
            android:defaultValue="0"
            android:key="@string/pref_max_bandwidth_key"
            android:title="@string/pref_max_bandwidth_title"></com.nexstreaming.app.apis.SliderPreference>

        <com.nexstreaming.app.apis.SliderPreference
            interval="1"
            max="10240"
            min="0"
            unitLabel="kbps"
            android:defaultValue="0"
            android:key="@string/pref_min_bandwidth_key"
            android:title="@string/pref_min_bandwidth_title"></com.nexstreaming.app.apis.SliderPreference>

        <com.nexstreaming.app.apis.ListPrefWithSummary
            android:defaultValue="0"
            android:entries="@array/pref_QACaptionMode_entries"
            android:entryValues="@array/pref_QACaptionMode_values"
            android:key="captionMode"
            android:summary="summary"
            android:title="@string/pref_captionMode"></com.nexstreaming.app.apis.ListPrefWithSummary>


        <com.nexstreaming.app.apis.ListPrefWithSummary
            android:defaultValue="0"
            android:entries="@array/pref_loglevel_values"
            android:entryValues="@array/pref_loglevel_values"
            android:key="logLevel"
            android:summary="summary"
            android:title="@string/pref_loglevel"></com.nexstreaming.app.apis.ListPrefWithSummary>

        <com.nexstreaming.app.apis.ListPrefWithSummary
            android:defaultValue="Auto"
            android:entries="@array/pref_renderMode_entries"
            android:entryValues="@array/pref_renderMode_values"
            android:key="renderMode"
            android:summary="summary"
            android:title="@string/pref_renderMode"></com.nexstreaming.app.apis.ListPrefWithSummary>


        <com.nexstreaming.app.apis.ListPrefWithSummary
            android:defaultValue="1"
            android:entries="@array/pref_colorSpace_entries"
            android:entryValues="@array/pref_colorSpace_values"
            android:key="colorSpace"
            android:summary="summary"
            android:title="@string/pref_colorSpace"></com.nexstreaming.app.apis.ListPrefWithSummary>

        <CheckBoxPreference
            android:defaultValue="true"
            android:key="eyePleaser"
            android:summary="@string/pref_eyePleaser_desc"
            android:summaryOff="@string/pref_eyePleaser_off"
            android:summaryOn="@string/pref_eyePleaser_on"
            android:title="@string/pref_eyePleaser"></CheckBoxPreference>

        <CheckBoxPreference
            android:defaultValue="false"
            android:key="StatisticsMonitor"
            android:summary="@string/pref_statistics_monitor_desc"
            android:summaryOff="@string/pref_statistics_monitor_off"
            android:summaryOn="@string/pref_statistics_monitor_on"
            android:title="@string/pref_statistics_monitor"></CheckBoxPreference>
        <com.nexstreaming.app.apis.SliderPreference
            interval="0.25"
            max="10"
            min="0"
            unitLabel="seconds"
            android:defaultValue="0"
            android:key="bufferTime"
            android:title="Buffering Time"></com.nexstreaming.app.apis.SliderPreference>

        <com.nexstreaming.app.apis.SliderPreference
            interval="0.05"
            max="1"
            min="-1"
            unitLabel="seconds"
            android:defaultValue="0"
            android:key="AVSyncOffset"
            android:title="AV Sync Offset"></com.nexstreaming.app.apis.SliderPreference>

        <com.nexstreaming.app.apis.SliderPreference
            interval="10"
            max="0"
            min="-100"
            unitLabel="msec"
            android:defaultValue="-50"
            android:key="VideoDisplayWait"
            android:title="Video Display Wait Offset"></com.nexstreaming.app.apis.SliderPreference>
        <com.nexstreaming.app.apis.SliderPreference
            interval="10"
            max="500"
            min="0"
            unitLabel="msec"
            android:defaultValue="70"
            android:key="VideoDisplaySkip"
            android:title="Video Display Skip Offset"></com.nexstreaming.app.apis.SliderPreference>
    </PreferenceScreen>

    <PreferenceScreen android:title="@string/dolby_ac3_setting">
        <com.nexstreaming.app.apis.ListPrefWithSummary
            android:defaultValue="0"
            android:entries="@array/pref_dolby_ac3_post_processing_entries"
            android:entryValues="@array/pref_dolby_ac3_end_point_entry_values"
            android:key="@string/dolby_ac3_post_processing"
            android:summary="summary"
            android:title="@string/dolby_ac3_post_processing"></com.nexstreaming.app.apis.ListPrefWithSummary>
        <com.nexstreaming.app.apis.ListPrefWithSummary
            android:defaultValue="0"
            android:entries="@array/pref_dolby_ac3_end_point_entries"
            android:entryValues="@array/pref_dolby_ac3_end_point_entry_values"
            android:key="@string/dolby_ac3_end_point"
            android:summary="summary"
            android:title="@string/dolby_ac3_end_point"></com.nexstreaming.app.apis.ListPrefWithSummary>
    </PreferenceScreen>

    <PreferenceScreen android:title="@string/dolby_ac4_setting">
        <com.nexstreaming.app.apis.ListPrefWithSummary
            android:defaultValue="0"
            android:entries="@array/pref_dolby_ac4_virtualization_entries"
            android:entryValues="@array/pref_dolby_ac4_virtualization_entry_values"
            android:key="@string/dolby_ac4_virtualization"
            android:summary="summary"
            android:title="@string/dolby_ac4_virtualization"></com.nexstreaming.app.apis.ListPrefWithSummary>
        <com.nexstreaming.app.apis.SliderPreference
            interval="1"
            max="12"
            min="0"
            android:defaultValue="0"
            android:key="@string/dolby_ac4_enhance_gain"
            android:title="@string/dolby_ac4_enhance_gain"></com.nexstreaming.app.apis.SliderPreference>
        <com.nexstreaming.app.apis.SliderPreference
            interval="1"
            max="32"
            min="-32"
            android:defaultValue="0"
            android:key="@string/dolby_ac4_main_asso_pref"
            android:title="@string/dolby_ac4_main_asso_pref"></com.nexstreaming.app.apis.SliderPreference>
        <com.nexstreaming.app.apis.SliderPreference
            interval="1"
            max="510"
            min="0"
            android:defaultValue="0"
            android:key="@string/dolby_ac4_presentation_index"
            android:title="@string/dolby_ac4_presentation_index"></com.nexstreaming.app.apis.SliderPreference>
    </PreferenceScreen>

    <PreferenceScreen android:title="Debug Log">
        <com.nexstreaming.app.apis.ListPrefWithSummary
            android:defaultValue="-1"
            android:entries="@array/pref_debug_logs_values"
            android:entryValues="@array/pref_debug_logs_values"
            android:key="@string/pref_debug_codec_key"
            android:summary="summary"
            android:title="@string/pref_debug_codec_title"></com.nexstreaming.app.apis.ListPrefWithSummary>

        <com.nexstreaming.app.apis.ListPrefWithSummary
            android:defaultValue="-1"
            android:entries="@array/pref_debug_logs_values"
            android:entryValues="@array/pref_debug_logs_values"
            android:key="@string/pref_debug_renderer_key"
            android:summary="summary"
            android:title="@string/pref_debug_renderer_title"></com.nexstreaming.app.apis.ListPrefWithSummary>

        <com.nexstreaming.app.apis.CheckBoxDialogPreference
            android:negativeButtonText="@string/cancel"
            android:positiveButtonText="@string/ok"
            android:title="@string/pref_debug_protocol_title" />

    </PreferenceScreen>

    <PreferenceScreen android:title="Trackdown">
        <CheckBoxPreference
            android:defaultValue="false"
            android:key="@string/pref_trackdownEnable_key"
            android:summary="@string/pref_trackdownEnable_desc"
            android:summaryOff="@string/pref_trackdownEnable_off"
            android:summaryOn="@string/pref_trackdownEnable_on"
            android:title="@string/pref_trackdownEnable"></CheckBoxPreference>

        <com.nexstreaming.app.apis.SliderPreference
            interval="1"
            max="100"
            min="0"
            unitLabel="percents"
            android:defaultValue="20"
            android:dependency="@string/pref_trackdownEnable_key"
            android:key="@string/pref_trackdownThreshold_key"
            android:title="Trackdown Threshold"></com.nexstreaming.app.apis.SliderPreference>
    </PreferenceScreen>

</PreferenceScreen>
