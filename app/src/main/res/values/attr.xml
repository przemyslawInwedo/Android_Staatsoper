<?xml version="1.0" encoding="utf-8"?>
<resources>
	<declare-styleable name="SliderPreference">
		<attr name="min" format="integer"/>
		<attr name="max" format="integer"/>
		<attr name="interval" format="integer"/>
	</declare-styleable>
	
	<declare-styleable name="EditTextFormat">
        <attr name="inputformat" format="string" />
        <attr name="completeformat" format="string" />
    </declare-styleable>

	<declare-styleable name="NexImageButton">
		<attr name="unfocused_backgroundColor" format="color" />
		<attr name="focused_backgroundColor" format="color" />
		<attr name="text" format="string"/>
		<attr name="textColor" format="color"/>
		<attr name="textSize" format="dimension"/>
		<attr name="textStrokeColor" format="color"/>
		<attr name="textStrokeWidth" format="float"/>
	</declare-styleable>

	<declare-styleable name="ThumbnailSeekBar">
		<attr name="backgroundColor" format="color" />
	</declare-styleable>
</resources>
